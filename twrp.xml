<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote name="TeamWin-Next"
            fetch="https://github.com/TWRP-Test"
            revision="refs/heads/twrp-16.0-next"
            review="https://gerrit.twrp.me"/>

    <remote name="LineageOS"
            fetch="https://github.com/LineageOS"
            review="https://review.lineageos.org"/>

    <remote name="github"
            fetch="https://github.com"/>

  <!-- Build -->
  <project path="build/make" name="android_build" groups="pdk,sysui-studio" remote="TeamWin-Next" >
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="android_build_soong" groups="pdk,tradefed,sysui-studio" remote="TeamWin-Next" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <!-- Bootable -->
  <project path="bootable/recovery" name="android_bootable_recovery" groups="pdk" remote="TeamWin-Next" />

  <!-- CTS -->
  <project path="cts" name="android_cts" groups="cts,pdk-cw-fs,pdk-fs" remote="TeamWin-Next" />

  <!-- External -->
  <project path="external/icu" name="android_external_icu" groups="pdk" remote="TeamWin-Next" />
  <project path="external/libchrome" name="android_external_libchrome" groups="pdk" remote="TeamWin-Next" />
  <project path="external/libxml2" name="android_external_libxml2" groups="pdk,libxml2" remote="TeamWin-Next" />
  <project path="external/sqlite" name="android_external_sqlite" groups="pdk" remote="TeamWin-Next" />

  <!-- Hardware -->
  <project path="hardware/interfaces" name="android_hardware_interfaces" groups="pdk,sysui-studio" remote="TeamWin-Next" />

  <!-- System -->
  <project path="system/core" name="android_system_core" groups="pdk" remote="TeamWin-Next" />
  <project path="system/extras" name="android_system_extras" groups="pdk" remote="TeamWin-Next" />
  <project path="system/libhidl" name="android_system_libhidl" groups="pdk,sysui-studio" remote="TeamWin-Next" />
  <project path="system/security" name="android_system_security" groups="pdk" remote="TeamWin-Next" />
  <project path="system/vold" name="android_system_vold" groups="pdk" remote="TeamWin-Next" />
  <project path="system/update_engine" name="android_system_update_engine" groups="pdk" remote="TeamWin-Next" />

  <!-- Vendor -->
  <project path="vendor/twrp" name="android_vendor_twrp" remote="TeamWin-Next" />
</manifest>